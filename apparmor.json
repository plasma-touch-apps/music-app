{
    "policy_version": 1.2,
    "policy_groups": [
        "audio",
        "content_exchange",
        "music_files_read",
        "networking",
        "usermetrics"
    ],
    "read_path": [
        "@{HOME}/.cache/media-art/",
        "@{HOME}/.cache/mediascanner-2.0/",
        "/media/*/*/"
    ],
    "write_path": [
        "@{HOME}/Music/Imported/"
    ]
}
